<h1><%= @user.first_name %> <%= @user.last_name %></h1>

<h2>Projects Owned:</h2>
<% @projects.each do |prj| %>
<div class="">
<%= link_to("#{prj.title}", project_path(prj)) %>
<% end %>
</div>

<h2>Projects Backed:</h2>
<% @pledges.each do |pld| %>
<div class="">
<%= link_to("#{pld.project.title}", project_path(pld.project)) %>
<% end %>
</div>
